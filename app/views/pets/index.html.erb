<h1 class="pet_color">All Pets</h1>
<% @pets.each do |pet| %>
    <h2><%= pet.name %></h2>
    <p>Breed: <%= pet.breed %></p>
    <p>Image: <%= pet.image %></p>
    <%= link_to "Pet Bio", pet %>
    <% end %>

    <style>
    .pet_color {
        color: blue;
    }

    </style>


    <%= link_to "New Pet", new_pet_path %>